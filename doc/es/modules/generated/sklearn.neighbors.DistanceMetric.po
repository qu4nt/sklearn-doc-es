msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:89
msgid "RogersTanimotoDistance"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:89
#: sklearn.neighbors._dist_metrics.DistanceMetric:91
msgid "2 * NNEQ / (N + NNEQ)"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:90
msgid "\"russellrao\""
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:90
msgid "RussellRaoDistance"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:90
msgid "NNZ / N"
msgstr ""

