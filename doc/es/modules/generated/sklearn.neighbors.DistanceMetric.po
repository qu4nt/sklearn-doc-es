msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:86
msgid "MatchingDistance"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:86
msgid "NNEQ / N"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:87
msgid "\"dice\""
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:87
msgid "DiceDistance"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:87
msgid "NNEQ / (NTT + NNZ)"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:88
msgid "\"kulsinski\""
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:88
msgid "KulsinskiDistance"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:88
msgid "(NNEQ + N - NTT) / (NNEQ + N)"
msgstr ""

