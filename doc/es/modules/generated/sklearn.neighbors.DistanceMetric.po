msgid "\"hamming\""
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:65
msgid "HammingDistance"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:65
msgid "``N_unequal(x, y) / N_tot``"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:66
msgid "\"canberra\""
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:66
msgid "CanberraDistance"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:66
msgid "``sum(|x - y| / (|x| + |y|))``"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:67
msgid "\"braycurtis\""
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:67
msgid "BrayCurtisDistance"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:67
msgid "``sum(|x - y|) / (sum(|x|) + sum(|y|))``"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:70
msgid "**Metrics intended for boolean-valued vector spaces:**  Any nonzero entry is evaluated to \"True\".  In the listings below, the following abbreviations are used:"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:74
msgid "N  : number of dimensions"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:75
msgid "NTT : number of dims in which both values are True"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:76
msgid "NTF : number of dims in which the first value is True, second is False"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:77
msgid "NFT : number of dims in which the first value is False, second is True"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:78
msgid "NFF : number of dims in which both values are False"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:79
msgid "NNEQ : number of non-equal dimensions, NNEQ = NTF + NFT"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:80
msgid "NNZ : number of nonzero dimensions, NNZ = NTF + NFT + NTT"
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:85
msgid "\"jaccard\""
msgstr ""

#: ../docstring of sklearn.neighbors._dist_metrics.DistanceMetric:85
