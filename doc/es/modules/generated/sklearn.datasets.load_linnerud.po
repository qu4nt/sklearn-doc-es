msgid "data_filename: str"
msgstr "data_filename: str"

#: of sklearn.datasets._base.load_linnerud:55
msgid "The path to the location of the data."
msgstr "La ruta de acceso a la ubicación de los datos."

#: of sklearn.datasets._base.load_linnerud:59
msgid "target_filename: str"
msgstr "target_filename: str"

#: of sklearn.datasets._base.load_linnerud:57
msgid "The path to the location of the target."
msgstr "La ruta de acceso a la ubicación del objetivo."

