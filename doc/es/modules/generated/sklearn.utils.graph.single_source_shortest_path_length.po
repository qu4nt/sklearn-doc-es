msgid "{sparse matrix, ndarray} of shape (n, n)"
msgstr "{sparse matrix, ndarray} de forma (n, n)"

#: of sklearn.utils.graph.single_source_shortest_path_length:9
msgid "Adjacency matrix of the graph. Sparse matrix of format LIL is preferred."
msgstr "Matriz de adyacencia del grafo. Se prefiere la Matriz Dispersa de formato LIL."

#: of sklearn.utils.graph.single_source_shortest_path_length:13
msgid "**source**"
msgstr "**source**"

#: of
msgid "int"
msgstr "int"

#: of sklearn.utils.graph.single_source_shortest_path_length:13
msgid "Starting node for path."
msgstr "Nodo inicial de la ruta o camino."

#: of sklearn.utils.graph.single_source_shortest_path_length:26
msgid "**cutoff**"
msgstr "**cutoff**"

#: of
msgid "int, default=None"
msgstr "int, default=None"

#: of sklearn.utils.graph.single_source_shortest_path_length:16
msgid "Depth to stop the search - only paths of length <= cutoff are returned."
msgstr "Profundidad para detener la búsqueda - sólo se devuelven las rutas de longitud <= cutoff."

