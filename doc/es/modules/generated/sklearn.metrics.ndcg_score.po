msgid "**k**"
msgstr "**k**"

#: of
msgid "int, default=None"
msgstr "int, default=None"

#: of sklearn.metrics._ranking.ndcg_score:24
msgid "Only consider the highest k scores in the ranking. If None, use all outputs."
msgstr ""

#: of sklearn.metrics._ranking.ndcg_score:28
msgid "**sample_weight**"
msgstr "**sample_weight**"

#: of
msgid "ndarray of shape (n_samples,), default=None"
msgstr "ndarray de forma (n_samples,), default=None"

#: of sklearn.metrics._ranking.ndcg_score:28
msgid "Sample weights. If None, all samples are given the same weight."
msgstr "Ponderaciones de muestras. Si es None, todas las muestras tienen la misma ponderación."

