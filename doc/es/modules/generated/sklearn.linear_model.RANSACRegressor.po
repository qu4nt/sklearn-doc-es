msgid "**inlier_mask_**"
msgstr ""

#: of
msgid "bool array of shape [n_samples]"
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:116
msgid "Boolean mask of inliers classified as ``True``."
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:121
msgid "**n_skips_no_inliers_**"
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:119
msgid "Number of iterations skipped due to finding zero inliers."
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:127
msgid "**n_skips_invalid_data_**"
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:124
msgid "Number of iterations skipped due to invalid data defined by ``is_data_valid``."
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:138
msgid "**n_skips_invalid_model_**"
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:130
msgid "Number of iterations skipped due to an invalid model defined by ``is_model_valid``."
msgstr ""

