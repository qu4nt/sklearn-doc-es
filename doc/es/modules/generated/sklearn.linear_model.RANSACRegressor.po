msgid "object"
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:109
msgid "Best fitted model (copy of the `base_estimator` object)."
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:113
msgid "**n_trials_**"
msgstr ""

#: of
msgid "int"
msgstr ""

#: of sklearn.linear_model._ransac.RANSACRegressor:112
msgid "Number of random selection trials until one of the stop criteria is met. It is always ``<= max_trials``."
msgstr ""

