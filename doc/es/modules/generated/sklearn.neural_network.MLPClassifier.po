msgid "**loss_**"
msgstr "**loss_**"

#: of
msgid "float"
msgstr "float"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:170
msgid "The current loss computed with the loss function."
msgstr "La pérdida actual calculada con la función de pérdida."

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:173
msgid "**best_loss_**"
msgstr "**best_loss_**"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:173
msgid "The minimum loss reached by the solver throughout fitting."
msgstr ""

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:176
msgid "**loss_curve_** : list of shape (`n_iter_`,)"
msgstr "**loss_curve_** : list de forma (`n_iter_`,)"

#: of
msgid "list of shape ("
msgstr "list de forma ("

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:176
msgid "The ith element in the list represents the loss at the ith iteration."
msgstr ""

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:179
msgid "**t_**"
msgstr "**t_**"

#: of
msgid "int"
msgstr "int"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:179
msgid "The number of training samples seen by the solver during fitting."
msgstr ""

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:183
msgid "**coefs_**"
msgstr "**coefs_**"

#: of
msgid "list of shape (n_layers - 1,)"
msgstr "list de forma (n_layers - 1,)"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:182
msgid "The ith element in the list represents the weight matrix corresponding to layer i."
msgstr ""

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:187
msgid "**intercepts_**"
msgstr "**intercepts_**"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:186
msgid "The ith element in the list represents the bias vector corresponding to layer i + 1."
msgstr ""

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:190
msgid "**n_iter_**"
msgstr "**n_iter_**"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:190
msgid "The number of iterations the solver has ran."
msgstr "El número de iteraciones que ha ejecutado el solucionador."

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:193
msgid "**n_layers_**"
msgstr "**n_layers_**"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:193
msgid "Number of layers."
msgstr "Número de capas."

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:196
msgid "**n_outputs_**"
msgstr "**n_outputs_**"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:196
msgid "Number of outputs."
msgstr "Número de salidas."

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:203
msgid "**out_activation_**"
msgstr "**out_activation_**"

#: of
msgid "str"
msgstr "str"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:199
msgid "Name of the output activation function."
msgstr "Nombre de la función de activación de salida."

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:206
msgid "Notes"
msgstr "Notas"

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:207
msgid "MLPClassifier trains iteratively since at each time step the partial derivatives of the loss function with respect to the model parameters are computed to update the parameters."
msgstr ""

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:211
msgid "It can also have a regularization term added to the loss function that shrinks model parameters to prevent overfitting."
msgstr ""

#: of sklearn.neural_network._multilayer_perceptron.MLPClassifier:214
msgid "This implementation works with data represented as dense numpy arrays or sparse scipy arrays of floating point values."
msgstr ""

