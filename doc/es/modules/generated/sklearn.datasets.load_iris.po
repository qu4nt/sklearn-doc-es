msgid "Samples per class"
msgstr "Muestras por clase"

#: of sklearn.datasets._base.load_iris:9
msgid "50"
msgstr "50"

#: of sklearn.datasets._base.load_iris:10
msgid "Samples total"
msgstr "Total de muestras"

#: of sklearn.datasets._base.load_iris:10
msgid "150"
msgstr "150"

#: of sklearn.datasets._base.load_iris:11
msgid "Dimensionality"
msgstr "Dimensionalidad"

