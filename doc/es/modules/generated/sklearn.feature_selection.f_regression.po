msgid "**F**"
msgstr "**F**"

#: of
msgid "array, shape=(n_features,)"
msgstr "array, shape=(n_features,)"

#: of sklearn.feature_selection._univariate_selection.f_regression:31
msgid "F values of features."
msgstr "Valores F de las características."

#: of sklearn.feature_selection._univariate_selection.f_regression:40
msgid "**pval**"
msgstr "**pval**"

#: of sklearn.feature_selection._univariate_selection.f_regression:34
msgid "p-values of F-scores."
msgstr "valores-p de las puntuaciones-F."

#: of sklearn.feature_selection._univariate_selection.f_regression:45
msgid ":obj:`mutual_info_regression`"
msgstr ":obj:`mutual_info_regression`"

#: of sklearn.feature_selection._univariate_selection.f_regression:46
msgid "Mutual information for a continuous target."
msgstr "Información mutua para un objetivo continuo."

#: of sklearn.feature_selection._univariate_selection.f_regression:47
msgid ":obj:`f_classif`"
msgstr ":obj:`f_classif`"

#: of sklearn.feature_selection._univariate_selection.f_regression:48
msgid "ANOVA F-value between label/feature for classification tasks."
msgstr "Valor-F de ANOVA entre etiqueta/característica para tareas de clasificación."

#: of sklearn.feature_selection._univariate_selection.f_regression:49
msgid ":obj:`chi2`"
msgstr ":obj:`chi2`"

#: of sklearn.feature_selection._univariate_selection.f_regression:50
msgid "Chi-squared stats of non-negative features for classification tasks."
msgstr "Estadísticas Chi-cuadrado de las características no negativas para las tareas de clasificación."

#: of sklearn.feature_selection._univariate_selection.f_regression:51
msgid ":obj:`SelectKBest`"
msgstr ":obj:`SelectKBest`"

#: of sklearn.feature_selection._univariate_selection.f_regression:52
msgid "Select features based on the k highest scores."
msgstr "Selecciona características basándose en las k puntuaciones más altas."

#: of sklearn.feature_selection._univariate_selection.f_regression:53
msgid ":obj:`SelectFpr`"
msgstr ":obj:`SelectFpr`"

#: of sklearn.feature_selection._univariate_selection.f_regression:54
